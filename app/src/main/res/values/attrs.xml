<?xml version="1.0" encoding="utf-8"?>
<resources>
    <attr name="startIcon" format="reference" />
    <attr name="firstText" format="string" />
    <attr name="secondText" format="string" />

    <declare-styleable name="SmsCodeEditText">
        <attr name="smsCodeAnimationType" format="enum">
            <enum name="popIn" value="0" />
            <enum name="fromBottom" value="1" />
            <enum name="none" value="-1" />
        </attr>
        <attr name="smsCodeCharacterMask" format="string" />
        <attr name="smsCodeRepeatedHint" format="string" />
        <attr name="smsCodeLineStroke" format="dimension" />
        <attr name="smsCodeLineStrokeSelected" format="dimension" />
        <attr name="smsCodeCharacterSpacing" format="dimension" />
        <attr name="smsCodeTextBottomPadding" format="dimension" />
        <attr name="smsCodeLineColors" format="color" />
        <attr name="smsCodeBackgroundDrawable" format="reference" />
        <attr name="smsCodeBackgroundIsSquare" format="boolean" />
        <attr name="smsCodeFont" format="string" />
    </declare-styleable>

    <declare-styleable name="PasswordEditText">
        <attr name="drawableShow" format="reference" />
        <attr name="drawableHide" format="reference" />
    </declare-styleable>

    <declare-styleable name="ButtonWithLoading">
        <attr name="textColor" format="reference|color" />
        <attr name="indicatorColor" format="reference|color" />
        <attr name="text" format="string" />
    </declare-styleable>

    <declare-styleable name="ButtonTwoValue">
        <attr name="firstTextStep" format="string" />
        <attr name="secondTextStep" format="string" />
        <attr name="firstTextColor" format="reference|color" />
        <attr name="secondTextColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="CustomItem">
        <attr name="startIcon" />
        <attr name="firstText" />
        <attr name="secondText" />
        <attr name="thirdText" format="string" />
    </declare-styleable>

    <declare-styleable name="DropUpDownItemView">
        <attr name="startIcon" />
        <attr name="firstText" />
        <attr name="secondText" />
        <attr name="isOpen" format="boolean" />
    </declare-styleable>

    <!-- Don't touch this -->
    <attr name="numberPickerStyle" format="reference" />
    <declare-styleable name="NumberPicker">
        <attr name="np_width" format="dimension" />
        <attr name="np_height" format="dimension" />
        <attr name="np_accessibilityDescriptionEnabled" format="boolean" />
        <attr name="np_divider" format="reference" />
        <attr name="np_dividerType" format="enum">
            <enum name="side_lines" value="0" />
            <enum name="underline" value="1" />
        </attr>
        <attr name="np_dividerColor" format="color" />
        <attr name="np_dividerDistance" format="dimension" />
        <attr name="np_dividerLength" format="dimension" />
        <attr name="np_dividerThickness" format="dimension" />
        <attr name="np_fadingEdgeEnabled" format="boolean" />
        <attr name="np_fadingEdgeStrength" format="float" />
        <attr name="np_formatter" format="string" />
        <attr name="np_hideWheelUntilFocused" format="boolean" />
        <attr name="np_itemSpacing" format="dimension" />
        <attr name="np_lineSpacingMultiplier" format="float" />
        <attr name="np_max" format="integer" />
        <attr name="np_maxFlingVelocityCoefficient" format="integer" />
        <attr name="np_min" format="integer" />
        <attr name="np_order" format="enum">
            <enum name="ascending" value="0" />
            <enum name="descending" value="1" />
        </attr>
        <attr name="np_orientation" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical" value="1" />
        </attr>
        <attr name="np_scrollerEnabled" format="boolean" />
        <attr name="np_selectedTextAlign" format="enum">
            <enum name="selectedTextAlignRight" value="0" />
            <enum name="selectedTextAlignCenter" value="1" />
            <enum name="selectedTextAlignLeft" value="2" />
        </attr>
        <attr name="np_selectedTextColor" format="color" />
        <attr name="np_selectedTextSize" format="dimension" />
        <attr name="np_selectedTextStrikeThru" format="boolean" />
        <attr name="np_selectedTextUnderline" format="boolean" />
        <attr name="np_selectedTypeface" format="string" />
        <attr name="np_textAlign" format="enum">
            <enum name="textAlignRight" value="0" />
            <enum name="textAlignCenter" value="1" />
            <enum name="textAlignLeft" value="2" />
        </attr>
        <attr name="np_textColor" format="color" />
        <attr name="np_textSize" format="dimension" />
        <attr name="np_textStrikeThru" format="boolean" />
        <attr name="np_textUnderline" format="boolean" />
        <attr name="np_typeface" format="string" />
        <attr name="np_value" format="integer" />
        <attr name="np_wheelItemCount" format="integer" />
        <attr name="np_wrapSelectorWheel" format="boolean" />
    </declare-styleable>

    <declare-styleable name="TitleErrorInputLayout">
        <attr name="editTextMargin" format="dimension" />
        <attr name="titleText" format="reference|string" />
        <attr name="titleTextColor" format="reference|color" />
    </declare-styleable>

    <declare-styleable name="BlockingProgressBar">
        <attr name="progressBarColor" format="color" />
    </declare-styleable>

    <declare-styleable name="OneElement">
        <attr name="backgroundClick" format="color"/>
        <attr name="backgroundUnClick" format="color"/>
        <attr name="isClick" format="boolean"/>
    </declare-styleable>

</resources>